section "Prepare Program & Domain"
stop at examples.tests.DataTest:28
run examples/tests/DataTest.java

section "Build Knowledge Base"
mapping set deep <<EOF
    examples.tests.DataContainer.intArrayField
    examples.tests.DataContainer.stringArrayField
EOF
buildkb
stats
checkkb --is-consistent

section "SPARQL Queries"

# See if all fields have been registered in the graph
sparql -- '<<EOF
    SELECT ?field
    WHERE {
        prog:examples.tests.DataContainer java:hasField ?field .
    }
'EOF

# Try to extract a boolean field value
sparql -- '<<EOF
    SELECT ?container ?boolVal
    WHERE {
        ?container a prog:examples.tests.DataContainer ;
                   prog:examples.tests.DataContainer.booleanField ?boolVal .
        FILTER (?boolVal = "true"^^xsd:boolean)
    }
'EOF
inspect ?boolVal

# Try to extract the second int array element
sparql -- '<<EOF
    SELECT ?second
    WHERE {
        ?container a prog:examples.tests.DataContainer ;
                   prog:examples.tests.DataContainer.intArrayField [
                     a prog:int%5B%5D ;
                     java:hasElement [
                         java:hasIndex 1 ;
                         java:storesPrimitive ?second
                     ]
                   ] .
    }
'EOF
inspect ?second

# Try to extract the first String array element
sparql -- '<<EOF
    SELECT ?first
    WHERE {
        [] a prog:examples.tests.DataContainer ;
           prog:examples.tests.DataContainer.stringArrayField [
             a prog:java.lang.String%5B%5D ;
             java:hasElement [
                 java:hasIndex 0 ;
                 java:storesReference [
                    java:hasPlainValue ?first
                 ]
             ]
           ] .
    }
'EOF
inspect ?first

# Show value of the string field
sparql -- '<<EOF
    SELECT ?string
    WHERE {
        [] a prog:examples.tests.DataContainer ;
           prog:examples.tests.DataContainer.stringField [
             java:hasPlainValue ?string
           ] .
    }
'EOF
inspect ?string

# Show the whole DataContainer instance
inspect ?container

stop at examples.tests.DataTest:44
cont
mapping set deep <<EOF
    examples.tests.DataContainer.intArrayField
    examples.tests.DataContainer.stringArrayField
    examples.tests.DataTest.void_varsTest().intArrayVar
EOF
buildkb
checkkb --is-consistent
stats

# Extracting a boolean value
sparql -- '<<EOF
    SELECT ?boolVal
    WHERE {
        [] a java:StackFrame ;
           prog:examples.tests.DataTest.void_varsTest%28%29.booleanVar ?boolVal .
        FILTER (?boolVal = "true"^^xsd:boolean)
    }
'EOF
inspect ?boolVal

# Extracting an array value
sparql -- '<<EOF
    SELECT ?intArrayVal
    WHERE {
        [] a java:StackFrame ;
           prog:examples.tests.DataTest.void_varsTest%28%29.intArrayVar [
             java:hasElement [
               java:hasIndex 2 ;
               java:storesPrimitive ?intArrayVal
             ]
           ] .
    }
'EOF
inspect ?intArrayVal

cont