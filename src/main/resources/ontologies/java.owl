###  https://github.com/ahbnr/SemanticJavaDebugger#hasField
java:hasField rdf:type owl:ObjectProperty ;
              rdfs:domain java:Class ;
              rdfs:range java:Field .


###  https://github.com/ahbnr/SemanticJavaDebugger#storesReferenceTo
java:storesReferenceTo rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( java:Field
                                                   java:Variable
                                                 )
                                   ] ;
                       rdfs:range java:Object .


