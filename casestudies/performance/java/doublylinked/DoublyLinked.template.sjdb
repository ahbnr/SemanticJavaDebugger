domain domain.ttl
classpaths './'
sourcepath 'DoublyLinked.java'
mapping set limit-sdk true
timeout {{timeout}}

stop at 'DoublyLinked:Nodes created'

{% for warmupIdx in range(0, warmup+1) %}
    {%- if warmupIdx == warmup %}
        memory clear
    {%- endif %}
    run -- DoublyLinked
    buildkb --linting=none

    {% include task + ".template.sjdb" %}
    kill
{% endfor %}

time --dump-tagged-to-json=times.json